{% extends "layout.twig"%}
{% block content %}
<div class="col-sm-10 сol-xs-12">
  {% for list in lists %}
    <div>
      <div class="col-xs-10">
      	<a href="/library/index.php?={{ list.id }}">{{ list.item }}</a>
      </div>
      <div class="col-xs-2">
      	<p>{{ list.numberOfBooks }}</p>
      </div>
    </div>
  {% endfor %}
</div> 
</div> 
{% endblock %}