{{ include ("head.twig") }}  
<div class="container">
  <form class="form-signin">
    <h2 class="form-signin-heading">Вход</h2>
    <input type="text" class="input-block-level" placeholder="Логин" required>
    <input type="text" class="input-block-level" placeholder="e-mail" required>
    <input type="password" class="input-block-level" placeholder="Пароль" required>
    <label class="checkbox">
      <input type="checkbox" value="remember-me"> Запомнить
    </label>
    <button class="btn btn-large btn-primary" type="submit">Sign in</button>
  </form>
</div>
{{ include("body_end.twig") }}